{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "",
    "projects": {
        "ng9": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "apps\\ng9",
            "sourceRoot": "apps\\ng9\\src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist\\apps\\ng9",
                        "index": "apps/ng9/src/index.html",
                        "main": "apps/ng9/src/main.ts",
                        "polyfills": "apps/ng9/src/polyfills.ts",
                        "tsConfig": "apps\\ng9\\tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "apps/ng9/src/favicon.ico",
                            "apps/ng9/src/assets"
                        ],
                        "styles": ["apps/ng9/src/styles.scss"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/ng9/src/environments/environment.ts",
                                    "with": "apps/ng9/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "ng9:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "ng9:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "ng9:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps\\ng9\\tsconfig.app.json",
                            "apps\\ng9\\tsconfig.spec.json",
                            "apps/ng9/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/ng9/jest.config.js",
                        "tsConfig": "apps/ng9/tsconfig.spec.json",
                        "setupFile": "apps/ng9/src/test-setup.ts",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "ng-effects": {
            "projectType": "library",
            "root": "libs/ng-effects",
            "sourceRoot": "libs/ng-effects/src",
            "prefix": "ng9",
            "architect": {
                "build": {
                    "builder": "@nrwl/angular:package",
                    "options": {
                        "tsConfig": "libs/ng-effects/tsconfig.lib.json",
                        "project": "libs/ng-effects/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "libs/ng-effects/tsconfig.lib.prod.json"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "libs/ng-effects/tsconfig.lib.json",
                            "libs/ng-effects/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!libs/ng-effects/**"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/ng-effects/jest.config.js",
                        "tsConfig": "libs/ng-effects/tsconfig.spec.json",
                        "setupFile": "libs/ng-effects/src/test-setup.ts",
                        "passWithNoTests": true
                    }
                }
            },
            "schematics": {}
        },
        "todomvc": {
            "projectType": "application",
            "schematics": {
                "@nrwl/angular:component": {
                    "inlineTemplate": true,
                    "inlineStyle": true
                }
            },
            "root": "apps/todomvc",
            "sourceRoot": "apps/todomvc/src",
            "prefix": "ngfx",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/todomvc",
                        "index": "apps/todomvc/src/index.html",
                        "main": "apps/todomvc/src/main.ts",
                        "polyfills": "apps/todomvc/src/polyfills.ts",
                        "tsConfig": "apps/todomvc/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "apps/todomvc/src/favicon.ico",
                            "apps/todomvc/src/assets"
                        ],
                        "styles": ["apps/todomvc/src/styles.css"],
                        "stylePreprocessorOptions": {
                            "includePaths": ["node_modules"]
                        },
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/todomvc/src/environments/environment.ts",
                                    "with": "apps/todomvc/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "todomvc:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "todomvc:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "todomvc:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/todomvc/tsconfig.app.json",
                            "apps/todomvc/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!apps/todomvc/**"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/todomvc/jest.config.js",
                        "tsConfig": "apps/todomvc/tsconfig.spec.json",
                        "passWithNoTests": true,
                        "setupFile": "apps/todomvc/src/test-setup.ts"
                    }
                }
            }
        }
    },
    "defaultProject": "ng9",
    "cli": {
        "analytics": false,
        "defaultCollection": "@nrwl/angular"
    },
    "schematics": {
        "@nrwl/angular:application": {
            "unitTestRunner": "jest",
            "e2eTestRunner": "none"
        },
        "@nrwl/angular:library": {
            "unitTestRunner": "jest"
        }
    }
}
